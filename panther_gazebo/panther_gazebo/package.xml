  <author email="krzysztof.wojciechowski@husarion.com">Krzysztof Wojciechowski</author>
  <license>Apache License 2.0</license>

  <url type="website">https://husarion.com/</url>
  <url type="repository">https://github.com/husarion/panther_simulation/tree/ros1</url>
  <url type="bugtracker">https://github.com/husarion/panther_simulation/issues</url>

  <buildtool_depend>catkin</buildtool_depend>
  <depend>controller_manager</depend>
  <depend>gazebo_plugins</depend>
  <depend>gazebo_ros</depend>
  <depend>gazebo_ros_control</depend>
  <depend>hector_gazebo_plugins</depend>
  <depend>joint_state_controller</depend>
